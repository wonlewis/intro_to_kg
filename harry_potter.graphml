<node id="n3495" labels=":Organization"><data key="labels">:Organization</data><data key="id">vault at Gringotts Wizarding Bank in London</data></node>
<node id="n3496" labels=":Person"><data key="labels">:Person</data><data key="id">his parents</data></node>
